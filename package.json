{
  "name": "@belongnet/metakeep-wagmi-connector",
  "version": "0.3.0",
  "description": "Metakeep connector for Wagmi",
  "keywords": [
    "metakeep",
    "wagmi",
    "connector"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/belongnet/metakeep-wagmi-connector.git"
  },
  "license": "MIT",
  "author": "reslear <reslear@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    }
  },
  "main": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "cd playground && pnpm dev",
    "build": "pnpm run clean && pnpm run build:esm+types",
    "build:esm+types": "tsc --outDir ./dist/esm --declaration --declarationMap --declarationDir ./dist/types",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "metakeep": "^2.2.2"
  },
  "peerDependencies": {
    "@wagmi/core": "2.x",
    "typescript": ">=5.0.4",
    "viem": "2.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "viem": "^2.10.9",
    "@wagmi/core": "2.10.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.1.0+sha512.67f5879916a9293e5cf059c23853d571beaf4f753c707f40cb22bed5fb1578c6aad3b6c4107ccb3ba0b35be003eb621a16471ac836c87beb53f9d54bb4612724"
}
