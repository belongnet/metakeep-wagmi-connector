{
  "name": "@reslear/metakeep-wagmi-connector",
  "version": "0.0.5",
  "description": "A connector for Metakeep and Wagmi",
  "keywords": [
    "metakeep",
    "wagmi",
    "connector"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/reslear/metakeep-wagmi-connector.git"
  },
  "license": "MIT",
  "author": "reslear <reslear@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "main": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run clean && pnpm run build:esm+types",
    "build:esm+types": "tsc --outDir ./dist/esm --declaration --declarationMap --declarationDir ./dist/types",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "metakeep": "^2.2.1"
  },
  "peerDependencies": {
    "@wagmi/core": "2.x",
    "typescript": ">=5.0.4",
    "viem": "2.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@wagmi/core": "2.6.16",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  }
}