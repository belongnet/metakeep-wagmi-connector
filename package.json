{
  "name": "@belongnet/metakeep-wagmi-connector",
  "version": "0.3.1",
  "description": "Metakeep connector for Wagmi",
  "keywords": [
    "metakeep",
    "wagmi",
    "connector"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/belongnet/metakeep-wagmi-connector.git"
  },
  "license": "MIT",
  "author": "reslear <reslear@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    }
  },
  "main": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts"
  ],
  "scripts": {
    "build": "pnpm run clean && pnpm run build:esm+types",
    "build:esm+types": "tsc --outDir ./dist/esm --declaration --declarationMap --declarationDir ./dist/types",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "dev": "cd playground && pnpm dev",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "metakeep": "^2.2.6"
  },
  "devDependencies": {
    "@wagmi/core": "2.13.3",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "viem": "^2.19.2"
  },
  "peerDependencies": {
    "@wagmi/core": "2.x",
    "typescript": ">=5.0.4",
    "viem": "2.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}